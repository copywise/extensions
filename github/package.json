      "conditionRule": "(https://github.com/|git@[\\w.-]+(?:\\.[\\w.-]+)+:)([\\w-]+)(?:/[\\w-]+)*/([\\.\\w-]+)(?:\\.git)?",
      "preferences": [
        {
          "name": "executableURL",
          "description": "",
