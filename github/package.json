{
  "name": "github",
  "version": "0.0.1",
  "title": "Github",
  "description": "",
  "scripts": {
    "build": "sh ./scripts/build.sh"
  },
  "peerDependencies": {
    "@copywise/api": "^0.0.4",
    "react": "^19.0.0"
  },
  "dependencies": {
    "@copywise/api": "0.0.7-alpha.2"
  },
  "devDependencies": {
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "esbuild": "^0.24.2",
    "typescript": "^4.9.5"
  },
  "icons": {
    "32": "assets/icon-32.png",
    "128": "assets/icon-128.png"
