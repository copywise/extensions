{
  "name": "cloudflare-r2",
  "version": "0.0.1",
  "title": "Cloudflare R2",
  "description": "Cloudflare R2 Extension: Upload to Cloudflare R2",
  "scripts": {
    "build": "copywise build --zip f1284293-89b2-48d5-9c88-0109a44105ec.zip"
  },
  "peerDependencies": {
    "@copywise/api": "0.0.8-alpha.7",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@copywise/cli": "0.0.2-alpha.4",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "esbuild": "^0.24.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "icons": {
    "32": "assets/icon-32.png",
    "128": "assets/icon-128.png"
  },
  "commands": [
    {
      "name": "Upload",
      "title": "Upload to Cloudflare R2",
      "condition": "file",
      "preferences": [
        {
          "name": "accessKey",
          "description": "Access Key ID",
          "label": "Access Key ID",
          "type": "password",
          "required": true
        },
        {
          "name": "secretKey",
          "description": "Secret Access Key",
          "label": "Secret Access Key",
          "type": "password",
          "required": true
        },
        {
          "name": "bucketName",
          "description": "Bucket name",
          "label": "Bucket name",
          "type": "text",
          "required": true
        },
        {
          "name": "endpoint",
          "description": "Endpoint",
          "label": "Endpoint",
          "type": "text",
          "required": true
        },
        {
          "name": "domain",
          "description": "Domain",
          "label": "Domain",
          "type": "text",
          "required": true
        },
        {
          "name": "folderPath",
          "description": "Folder path",
          "label": "Folder path",
          "type": "text"
        }
      ]
    }
  ],
  "packageManager": "pnpm@8.15.1+sha1.8adba2d20330c02d3856e18c4eb3819d1d3ca6aa",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@types/node": "^22.14.1"
  }
}